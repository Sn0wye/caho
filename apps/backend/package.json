{
  "name": "@caho/backend",
  "version": "1.0.50",
  "main": "dist/src/server.js",
  "types": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx --watch src/server.ts",
    "start": "tsx src/server.ts",
    "test": "vitest",
    "studio": "dotenv drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:push": "dotenv drizzle-kit push:mysql",
    "db:generate": "dotenv drizzle-kit generate:mysql",
    "db:migrate": "tsx src/db/migrate.ts",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "git clean -xdf node_modules"
  },
  "dependencies": {
    "@caho/contracts": "workspace:*",
    "@caho/schemas": "workspace:*",
    "@faker-js/faker": "^8.1.0",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/one-line-logger": "^1.2.0",
    "@fastify/sensible": "^5.3.0",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@lucia-auth/adapter-mysql": "^2.0.0",
    "@lucia-auth/oauth": "^3.2.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@planetscale/database": "^1.11.0",
    "@sinclair/typebox": "^0.31.17",
    "@socket.io/redis-adapter": "^8.2.1",
    "@t3-oss/env-core": "^0.6.1",
    "@upstash/redis": "^1.22.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "fastify": "^4.24.1",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.3.2",
    "ioredis-mock": "^8.9.0",
    "lucia": "^2.7.1",
    "mysql2": "^3.6.2",
    "socket.io": "^4.7.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@sn0wye/eslint-config": "^3.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.95",
    "@types/ioredis-mock": "^8.2.5",
    "@types/node": "^18.16.0",
    "@types/ws": "^8.5.10",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.50.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.1"
  }
}
