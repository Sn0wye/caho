{
  "name": "@caho/backend",
  "version": "1.0.50",
  "main": "dist/src/server.js",
  "types": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "dotenv tsx watch src/server.ts",
    "start": "node --env-file=.env dist/server.js",
    "build": "tsup",
    "test": "vitest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "lint": "biome lint src",
    "lint:fix": "biome lint src --apply",
    "typecheck": "tsc --noEmit",
    "clean": "git clean -xdf node_modules",
    "NOTE": "You can apply both linter, formatter and import ordering by using https://biomejs.dev/reference/cli/#biome-check"
  },
  "dependencies": {
    "@caho/contracts": "workspace:*",
    "@caho/schemas": "workspace:*",
    "@faker-js/faker": "^8.4.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^8.5.0",
    "@fastify/error": "^3.4.1",
    "@fastify/one-line-logger": "^1.2.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^3.6.0",
    "@fastify/websocket": "^9.0.0",
    "@lucia-auth/adapter-drizzle": "^1.0.4",
    "@lucia-auth/oauth": "^3.5.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sinclair/typebox": "^0.31.28",
    "@t3-oss/env-core": "^0.6.1",
    "@upstash/redis": "^1.28.3",
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.30.10",
    "fastify": "^4.26.0",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
    "ioredis": "^5.3.2",
    "ioredis-mock": "^8.9.0",
    "lucia": "^3.0.1",
    "mysql2": "^3.9.1",
    "oslo": "^1.0.4",
    "pg": "^8.11.5",
    "ws": "^8.17.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.4.0",
    "@types/ioredis-mock": "^8.2.5",
    "@types/node": "^20.12.8",
    "@types/pg": "^8.11.5",
    "@types/ws": "^8.5.10",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.21.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  }
}
