{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env", "tsconfig.json"],
  "pipeline": {
    "db:seed": {},
    "db:migrate": {},
    "db:generate": {},
    "start": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist"]
    },
    "check": {
      "cache": false
    },
    "check:fix": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
